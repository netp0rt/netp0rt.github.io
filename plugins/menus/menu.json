{
    "opts": {
        "name": "RTCMenu",
        "version": "1.0.0-FF",
        "description": "RighT Click Menu Plugin",
        "readme": "readme.md",
        "defaultSet": false,
        "isdev": false,
        "rootOpt": ""
    },
    "styles": "menu.css",
    "menus": {
        "#public":[
            {
                "name": "Public Example 1",
                "id": "public.example",
                "icon": "@fa:fa-solid fa-1",
                "action": "this.tst.s('Example 1 toast test')",
                "exc": ["mainMenu"]
            },
            {
                "name": "Public Submenu",
                "id": "public.submenu",
                "icon": "^https://www.emojiall.com/images/240/apple/1f600.png",
                "exc": ["mainMenu"],
                "submenus": [
                    {
                        "name": "Public Submenu",
                        "id": "public.submenu",
                        "icon": "^https://www.emojiall.com/images/240/apple/1f699.png",
                        "action": "this.tst.i(this.runScripts.debug())"
                    }
                ]
            }
        ],
        "mainMenu": [
            {
                "name": "mainMenu Example 1",
                "id": "main.example",
                "icon": "@fa:fa-solid fa-1",
                "action": "this.tst.s('mainMenu 1 toast test')"
            }
        ]
    },
    "trigger": {
        ".example": "mainMenu"
    }
}
